{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowS3AnsibleTestBuckets",
            "Action": [
                "s3:DeleteAccessPoint",
                "s3:DeleteAccessPointPolicy",
                "s3:DeleteBucketWebsite",
                "s3:ListAccessPoints",
                "s3:ListJobs",
                "s3:ListMultipartUploadParts"
            ],
            "Effect": "Allow",
            "Resource": [
                "arn:aws:s3:::ansible-test-*",
                "arn:aws:s3:::ansible-test-*/*"
            ]
        },
        {
            "Sid": "ManageEFS",
            "Effect": "Allow",
            "Action": [
                "elasticfilesystem:*"
            ],
            "Resource": "*"
        }
    ]
}
